{
  "name": "sg-health",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode=local --portal=admin",
    "serve-auth": "vue-cli-service serve --mode=local --portal=auth",
    "update-plugins": "yarn update-vuejs-helper && yarn update-lq-form && yarn update-lq-element && yarn update-lq-table &&  node plugins_update",
    "build-admin-local": "yarn update-plugins && yarn build-admin-local-only",
    "build-auth-local": "yarn update-plugins && yarn build-auth-local-only",
    "build-auth-local-only": "vue-cli-service build --mode=production --portal=auth",
    "build-admin-local-only": "vue-cli-service build --mode=production --portal=admin",
    "build-dev": "vue-cli-service build --mode=development",
    "test:e2e": "vue-cli-service test:e2e",
    "update-vuejs-helper": "cd plugins/vuejs-object-helper && yarn build-bundle",
    "update-lq-form": "cd plugins/vuex-lq-form && yarn update-plugins && yarn build-bundle",
    "update-lq-element": "cd plugins/vuex-lq-form-element && yarn update-plugins  && yarn build-bundle",
    "update-lq-table": "cd plugins/vuex-lq-table && yarn update-plugins && yarn build-bundle",
    "test:unit": "vue-cli-service test:unit",
    "svg": "vsvg  -s src/icons -t src/components/icons"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "copy-dir": "^0.4.0",
    "install": "^0.12.2",
    "js-cookie": "^2.2.0",
    "js-file-download": "^0.4.5",
    "lq-element-ui": "^2.7.3",
    "minify": "^4.1.1",
    "minimist": "^1.2.0",
    "normalize.css": "^8.0.1",
    "npm": "^6.9.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^3.0.0",
    "recursive-copy": "^2.0.9",
    "register-service-worker": "^1.5.2",
    "svg-sprite-loader": "^4.1.3",
    "url-polyfill": "^1.1.3",
    "vue": "^2.6.7",
    "vue-context": "^3.4.0",
    "vue-i18n": "^8.7.0",
    "vue-json-pretty": "^1.4.1",
    "vue-router": "^3.0.1",
    "vue-svg-loader": "^0.12.0",
    "vue-svgicon": "^3.2.2",
    "vue2-dropzone": "^3.5.8",
    "vuejs-object-helper": "^0.1.20",
    "vuex": "^3.0.1",
    "vuex-lq-form": "^0.1.59",
    "vuex-lq-form-element": "^0.1.9",
    "vuex-lq-table": "^0.1.26"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-pwa": "^3.2.0",
    "@vue/cli-plugin-unit-mocha": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.7"
  }
}
